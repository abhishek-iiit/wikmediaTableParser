# Source URL of the Wikipedia page
PAGE_URL=https://en.wikipedia.org/wiki/List_of_FIFA_World_Cup_finals

# DOM path to locate the main table (CSS selector)
TABLE_SELECTOR=#mw-content-text > div.mw-parser-output > table:nth-child(17)

# Number of rows to extract (e.g., first 10 finals)
ROW_COUNT=10

# Google Sheets settings
SPREADSHEET_ID=abcefghijklmnopqrstuvwxyz1234567890
SHEET_NAME=Finals
RANGE=${SHEET_NAME}!A1:D1

# Google API settings
GOOGLE_SHEETS_API_URL=https://sheets.googleapis.com/v4/spreadsheets
API_ENDPOINT=${GOOGLE_SHEETS_API_URL}/${SPREADSHEET_ID}/values/${RANGE}:append
VALUE_INPUT_OPTION=RAW

# Authentication
ACCESS_TOKEN=abcefghijklmnopqrstuvwxyz1234567890